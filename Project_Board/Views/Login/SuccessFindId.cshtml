
@{
    ViewBag.Title = "Home Page";
}

<head>
    <link rel="stylesheet" type="text/css" href="~/Content/Css/Login_FindImfor.css"/>
</head>

<h1 class="titleH1">ID情報</h1>

<div>
    @foreach (var item in Model)
    {
        <span class="itemNameResult">@item.Name</span>
        <span class="text">様の</span>
        <br />
        <span class="Passwordtext" style="margin: auto auto auto 30%; ">IDは、「 </span>
        <span class="itemPasswordResult" style="color: #0000cd;">@item.Id </span>
        <span class="text">」です。</span>
    }
</div>

<div class="form-group"">
    <a class="btn btn-warning" href="https://localhost:44342/">ホーム</a>
</div>