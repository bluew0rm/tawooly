
@{
    ViewBag.Title = "Home Page";
}
@*会員情報*@



@*<h2>@ViewData["DB接続"]</h2>*@
<h2>@ViewData["DB接続不可"]</h2>



<h1 style="text-align:center ; margin: 90px 0 0 0">会員情報</h1>




<table class="table" style="margin: 80px 0 0 0;">
    <thead style="text-align: center">
        <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>AGE</th>
            <th>GENDER</th>
            <th>JOB</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Gender</td>
                <td>@item.Job</td>
            </tr>
        }
    </tbody>
</table>

